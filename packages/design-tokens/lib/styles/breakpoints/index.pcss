/*! Phone: 0 <- 600px */
@custom-media --break-sm (max-width: 37.5em);
/*! Tablet portrait: 600px <- 900px */
@custom-media --break-md-port (max-width: 56.25em);
/*! Tablet landscape: 900px <- 1200px */
@custom-media --break-md-land (max-width: 75em);
/*! Desktop: 1200px <- 1800px */
@custom-media --break-lg (max-width: 112.5em);
