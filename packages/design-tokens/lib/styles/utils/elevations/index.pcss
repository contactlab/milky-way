@import '../../elevations/index.pcss';

.elevation-transition {
  transition: var(--elevation-transition);
}

@each $type in z1, z2, z3, z4, z5 {
  .elevation-$(type) {
    box-shadow: var(--elevation-$(type)) !important;
  }
  .elevation-accent-$(type) {
    box-shadow: var(--elevation-accent-$(type)) !important;
  }
}
