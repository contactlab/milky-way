{
  "name": "@contactlab/ds-tokens",
  "version": "0.4.3",
  "description": "Named entities that store visual-design info, in order to maintain a scalable, consistent system for user interface development.",
  "author": {
    "name": "Contactlab UXD Team",
    "email": "uxd@contactlab.com",
    "homepage": "https://ux.contactlab.com/"
  },
  "contributors": [
    "Carlo Audino <carlo.audino@contactlab.com>",
    "Giovanni Tramutola <giovanni.tramutola@contactlab.com>"
  ],
  "homepage": "https://github.com/contactlab/milky-way#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/contactlab/milky-way/tree/master/packages/design-tokens"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "./*.md",
    "LICENSE"
  ],
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "lint:css": "stylelint ./lib/**/*.{css,scss,less,svg} --fix",
