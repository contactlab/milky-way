{
  "name": "@contactlab/ds-tokens",
  "version": "1.0.0",
  "description": "Named entities that store visual-design info, in order to maintain a scalable, consistent system for user interface development.",
  "author": {
    "name": "Contactlab UXD Team",
    "email": "uxd@contactlab.com",
    "homepage": "https://ux.contactlab.com/"
  },
  "contributors": [
    "Carlo Audino <carlo.audino@contactlab.com>",
    "Giovanni Tramutola <giovanni.tramutola@contactlab.com>"
  ],
  "homepage": "https://github.com/contactlab/milky-way#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contactlab/milky-way/tree/master/packages/design-tokens"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "artworks/",
    "styles/"
  ],
  "scripts": {
    "setup": "node ./scripts/setup.js",
    "lint:css": "stylelint ./**/*.{css,scss,less,svg} --fix",
    "lint:src": "eslint . --fix",
    "test": "run-p -clns lint:**",
    "build:svg": "svgo --folder ./artworks/ --output ./artworks/min/ --disable inlineStyles",
    "prepublish": "run-s -lns test build:svg"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "2.2.3",
    "eslint": "6.8.0",
    "eslint-config-contactlab": "4.2.2",
    "eslint-config-prettier": "6.10.1",
    "npm-run-all": "4.1.5",
    "shelljs": "0.8.3",
    "stylelint": "13.3.0",
    "stylelint-scss": "3.16.0",
    "svgo": "1.3.2"
  }
}
