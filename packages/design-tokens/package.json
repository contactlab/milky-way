    "build:svg": "svgo --folder ./lib/illustrations/ --output ./lib/illustrations/min/ --disable inlineStyles",
    "build": "run-s setup build:**"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "2.1.1",
    "eslint": "6.8.0",
    "eslint-config-contactlab": "4.2.2",
    "eslint-config-prettier": "6.10.0",
    "npm-run-all": "4.1.5",
    "shelljs": "0.8.3",
    "stylelint": "13.2.1",
    "stylelint-scss": "3.15.0",
    "svgo": "1.3.2"
  }
