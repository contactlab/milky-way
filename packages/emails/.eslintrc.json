{
  "root": true,

  "plugins": ["react"],

  "extends": ["contactlab/typescript", "prettier"],

  "rules": {
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {"{}": false},
        "extendDefaults": true
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-vars": "error",
    "react/prop-types": "off",
    "react/display-name": "off"
  },

  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
