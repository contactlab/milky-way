{
  "private": true,
  "name": "@contactlab/emails",
  "version": "1.0.0",
  "description": "The MailUp CDP transactional emails.",
  "author": "Contactlab",
  "homepage": "https://github.com/contactlab/milky-way#readme",
  "repository": "contactlab/milky-way",
  "license": "MIT",
  "scripts": {
    "check": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "test": "npm run check && npm run lint",
    "prebuild": "rm -rf ./build",
    "build": "babel ./src --out-dir ./build --extensions .ts,.tsx",
    "watch": "npm run build -- --watch --source-maps inline",
    "pregenerate": "rm -rf ./public/emails/* && npm run build",
    "generate": "node build/generate.js",
    "preserve": "npm run generate",
    "serve": "http-server ./public -p 3333 --silent",
    "dev": "watch & serve"
  },
  "dependencies": {
    "@faire/mjml-react": "3.3.0",
    "mjml": "4.14.1",
    "node-polyglot": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.10",
    "@babel/plugin-transform-react-jsx": "7.22.5",
    "@babel/plugin-transform-typescript": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@types/mjml": "4.7.1",
    "@types/node": "20.5.0",
    "@types/node-polyglot": "2.4.2",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "eslint": "8.47.0",
    "eslint-config-contactlab": "11.2.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.33.2",
    "http-server": "14.1.1",
    "typescript": "5.1.6"
  }
}
