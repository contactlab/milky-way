{
  "private": true,
  "name": "@contactlab/emails",
  "version": "1.0.0",
  "description": "The MailUp CDP transactional emails.",
  "author": "Contactlab",
  "homepage": "https://github.com/contactlab/milky-way#readme",
  "repository": "contactlab/milky-way",
  "license": "MIT",
  "scripts": {
    "check": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "test": "npm run check && npm run lint",
    "prebuild": "rm -rf ./build",
    "build": "babel ./src --out-dir ./build --extensions .ts,.tsx",
    "watch": "npm run build -- --watch --source-maps inline",
    "preserve": "npm run build",
    "serve": "http-server ./public -p 3333 --silent",
    "dev": "watch & serve",
    "pregenerate": "rm -rf ./public/emails/* && npm run build",
    "generate": "node build/generate.js"
  },
  "dependencies": {
    "mjml": "4.10.4",
    "mjml-react": "2.0.2",
    "node-polyglot": "2.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-react-jsx": "7.16.0",
    "@babel/plugin-transform-typescript": "7.16.1",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@types/mjml-react": "2.0.1",
    "@types/node": "16.11.9",
    "@types/node-polyglot": "2.4.2",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "eslint": "7.32.0",
    "eslint-config-contactlab": "7.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.25.3",
    "http-server": "14.1.1",
    "typescript": "4.4.4"
  }
}
