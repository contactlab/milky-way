{
  "private": true,
  "name": "@contactlab/service-pages",
  "version": "1.7.0",
  "description": "The Contactlab Marketing Cloud service pages.",
  "author": {
    "name": "Contactlab UXD Team",
    "email": "uxd@contactlab.com",
    "homepage": "https://ux.contactlab.com"
  },
  "contributors": [
    "Carlo Audino <carlo.audino@contactlab.com>",
    "Giovanni Tramutola <giovanni.tramutola@contactlab.com>"
  ],
  "homepage": "https://github.com/contactlab/milky-way#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contactlab/milky-way/tree/master/packages/service-pages"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "scripts": {
    "check:src": "tsc --noEmit",
    "lint:css": "stylelint ./src/**/*.scss --ignore-path .gitignore",
    "lint:src": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lint": "run-p -clns lint:**",
    "dev": "parcel ./src/index.pug --port 3333 --target=browser --log-level 3",
    "prebuild": "rimraf ./dist",
    "build": "parcel build ./src/index.pug --target=browser --no-cache --no-source-maps --log-level 3",
    "predeploy": "run-s lint build",
    "deploy": "netlify deploy --dir ./dist --prod"
  },
  "dependencies": {
    "@contactlab/ds-tokens": "^1.7.0"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "5.1.0",
    "eslint": "7.12.1",
    "eslint-config-contactlab": "5.0.0",
    "eslint-config-prettier": "6.13.0",
    "netlify-cli": "2.65.6",
    "npm-run-all": "4.1.5",
    "parcel": "1.12.4",
    "pug": "3.0.0",
    "rimraf": "3.0.2",
    "sass": "1.27.0",
    "stylelint": "13.7.2",
    "stylelint-scss": "3.18.0",
    "typescript": "4.0.3"
  }
}
