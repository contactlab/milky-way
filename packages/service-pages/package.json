  },
  "license": "MIT",
  "engines": {
    "node": ">=10.18",
    "npm": ">=6.13"
  },
  "scripts": {
    "lint:css": "stylelint ./src/**/*.scss --fix",
    "lint:src": "eslint . --ext .js,.ts --fix",
    "test": "run-p lint:**",
    "dev": "parcel ./src/index.pug --port 8081 --target=browser --log-level 3",
    "prebuild": "rimraf ./dist",
    "build": "parcel build ./src/index.pug --target=browser --no-cache --no-source-maps --log-level 3",
    "serve": "serve -l 8180 ./dist",
    "predeploy": "run-s test build",
    "deploy": "netlify deploy --dir ./dist --prod"
  },
  "dependencies": {
    "@contactlab/ds-tokens": "*"
  },
  "devDependencies": {
    "@giotramu/stylelint-config": "2.2.3",
    "eslint": "6.8.0",
    "eslint-config-contactlab": "4.2.2",
    "eslint-config-prettier": "6.10.1",
    "netlify-cli": "2.40.0",
    "npm-run-all": "4.1.5",
    "parcel": "1.12.4",
    "pug": "2.0.4",
    "rimraf": "3.0.2",
    "sass": "1.26.3",
    "serve": "11.3.0",
    "stylelint": "13.2.1",
    "stylelint-scss": "3.16.0",
    "typescript": "3.8.3"
  }
